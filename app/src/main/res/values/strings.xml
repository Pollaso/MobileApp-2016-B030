    <string name="name_regex">^\\p{L}+[\\p{L}\\p{Zs}]{0,50}$</string>
    <string name="last_name_regex">^\\p{L}+[\\p{L}\\p{Zs}\\p{Pd}\\p{Pi}]{0,50}$</string>
    <string name="car_plate_regex">^[\\p{L}\\p{N}]{3,10}$</string>

